<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>insertdata</servlet-name>
        <servlet-class>com.register.insertdata</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OnwerData</servlet-name>
        <servlet-class>com.register.OnwerData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewGraph</servlet-name>
        <servlet-class>com.graph.ViewGraph</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewENCGraph</servlet-name>
        <servlet-class>com.graph.ViewENCGraph</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewDESGraph</servlet-name>
        <servlet-class>com.graph.ViewDESGraph</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Upload1</servlet-name>
        <servlet-class>com.fileimage.Upload1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>insertdata</servlet-name>
        <url-pattern>/insertdata</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OnwerData</servlet-name>
        <url-pattern>/OnwerData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewGraph</servlet-name>
        <url-pattern>/ViewGraph</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewENCGraph</servlet-name>
        <url-pattern>/ViewENCGraph</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewDESGraph</servlet-name>
        <url-pattern>/ViewDESGraph</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Upload1</servlet-name>
        <url-pattern>/Upload1</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
